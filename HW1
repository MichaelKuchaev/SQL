SELECT * FROM home_work_1.mobile_phones;

SELECT ProductName, Manufacturer, Price FROM home_work_1.mobile_phones
where ProductCount>2; #Выведите название, производителя и цену для товаров, количество которых превышает 2

SELECT * FROM home_work_1.mobile_phones
where Manufacturer='Samsung'; #Выведите весь ассортимент товаров марки “Samsung”

SELECT * FROM home_work_1.mobile_phones
where ProductName LIKE '%Iphone%'; #С помощью регулярных выражений найти: Товары, в наименовании которых есть упоминание "Iphone"

SELECT * FROM home_work_1.mobile_phones
where Manufacturer LIKE '%Samsung%'; #С помощью регулярных выражений найти: Товары, в наименовании которых есть упоминание "Samsung"

SELECT * FROM home_work_1.mobile_phones
where ProductName RLIKE '[0-9]'; #С помощью регулярных выражений найти: Товары, в наименовании которых есть ЦИФРЫ

SELECT * FROM home_work_1.mobile_phones
where ProductName RLIKE '[8]'; #С помощью регулярных выражений найти: Товары, в наименовании которых есть ЦИФРА "8"
